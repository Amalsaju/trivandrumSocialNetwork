<div class="container">
  <div class="row " style="padding-top: 20px;">
    <div class="col-lg-12 col-lg-offset-4 " style="width: 35%;">
      <h1 >Sign Up</h1>
        <form role="form" action="signUpSubmit" method="GET">
          <div class="form-group">
            <input type="text" name="name" id="name" class="form-control" id="exampleInputEmail1"  placeholder="Name" autofocus>
          </div>
