            <input type="email" name="email" id="email" class="form-control" id="exampleInputEmail1"  placeholder="Email" required autocomplete="email">
          </div>
          <div class="form-group">
            <input type="password" name="password" id="password" class="form-control" id="exampleInputEmail1"  placeholder="Password" required autocomplete="password">
